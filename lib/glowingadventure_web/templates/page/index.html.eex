<%= form_for @conn, Routes.analyze_path(@conn, :analyze), [multipart: true], fn f -> %>

<div class="form-group">
    <%= label f, :file, class: "control-label" %>
    <%= file_input f, :file, class: "form-control" %>
</div>

<div class="form-group">
    <%= submit "Submit", class: "button" %>
</div>
<% end %>
